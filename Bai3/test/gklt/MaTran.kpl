Program MaTran;
Var n:integer;
    a: array (.100.) of array (.100.) of integer;
    I: integer;
    J: integer;
    ch:Interger;
Begin
n:= readI;
ch:=1;
for I:= 1 to N do
for J:=1 to N do
    a(.i.)(.J.):= readI;
for I:= 1 to N do 
for J:=I to 1 do
if a(.I.)(.J.) != 0 Then ch:=0;
Call WriteI(ch); 
end.